<!doctype html>
<html>
<head>
<title>Qunar管理员后台</title>
<meta name="keywords" content="Qunar管理员后台">
<meta name="description" content="Qunar管理员后台">
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<link rel="stylesheet" type="text/css" href="http://q.qunarzz.com/piao_tts/prd/styles/v2/list@12d870584660f131bd2807926611a4c7.css">
</head>
<body>
<h3 class="caption"><i class="sign"></i>管理员帐号</h3>
<div class="list_condition">
	<div class="search clrfix">
		<form id="search-form" method="get" action="/admin/authority/queryAdmainAuthorityList">
			<table style="table-layout: fixed; width:948px;">
                <tr>
                    <td width="60" class="align_right"><label class="unit bold">名称</label></td>
                    <td width="60" class="align_right"><label for="username">账号：</label></td>
                    <td width="120">
                        <input id="username" name="username" class="fill" style="width: 100px" type="text"/>
                    </td>
                    <td width="60" class="align_right"><label for="name" >姓名：</label></td>
                    <td width="120">
                        <input class="fill" name="name" id="name" style="width: 100px" type="text"/>
                    </td>
                    <td width="60" class="align_right"><label for="rtxAccountPms">RTX：</label></td>
                    <td width="120">
                        <input class="fill" name="rtxAccountPms" value="" style="width: 100px" type="text"/>
                    </td>
                    <td width="120" class="align_right"> <select style="width: 80px" name="" id="shopSupplier">
                        <option value="shop">店铺名称</option>
                        <option value="supplier">商家名称</option>
                    </select></td>
                    <td>
                        <input id="shopsuggest" class="fill" style="width: 200px" type="text"/>
                        <input type="hidden" name="supplierId" id="supplierId" value=""/>
                    </td>
                </tr>
				<tr>
					<td class="align_right" valign="top"><label class="unit bold">用户角色</label></td>
					<td colspan="7">
						<div class="search_item clrfix" style="padding-left: 19px">
							<div class="search_group_list">
								<label for="roleIds_2" hidefocus="true">
									<input type="checkbox" value="2" name="roleIds[]" id="roleIds_2">
									供应商管理员</label>
							</div>
                            <div class="search_group_list">
                                <label for="roleId_100000006" hidefocus="true">
                                    <input type="checkbox" value="100000006" name="roleIds[]" id="roleId_100000006">
                                    分销商管理员</label>
                            </div>
                            <div class="search_group_list">
                                <label for="roleId_100000007" hidefocus="true">
                                    <input type="checkbox" value="100000007" name="roleIds[]" id="roleId_100000007">
                                    供应商+分销商管理员</label>
                            </div>
						</div>
					</td>
				</tr>
                <tr>
                    <td class="align_right"><label class="unit bold">状态</label></td>
                    <td colspan="7">
                        <label style="margin-left: 14px;" for="activeStatusPmsALL"><input id="activeStatusPmsALL" name="activeStatusPms" value="ALL" type="radio" checked="checked" class="input_radio"/> 全部</label>
                        <label for="activeStatusPmsUSING"><input id="activeStatusPmsUSING" type="radio" name="activeStatusPms" value="USING" class="input_radio"/> 启用</label>
                        <label for="activeStatusPmsFORBIDDEN"><input id="activeStatusPmsFORBIDDEN" type="radio" name="activeStatusPms" value="FORBIDDEN" class="input_radio"/> 禁用</label>
                    </td>
                </tr>
                <tr>
                    <td colspan="8" style="text-align:right;"><a href="javascript:void(0)" style="width:60px;" hidefocus="true" id="submitbutton" class="btn btn_orange">查询</a></td>
                </tr>
			</table>
		</form>
	</div>
</div>
<div class="list_cont">
	<div id="list-container"></div>
	<div id="pagination"></div>
</div>
<script src="http://q.qunarzz.com/jquery/prd/jquery-1.7.2.min.js" type="text/javascript" charset="utf-8"></script>
<script src="http://q.qunarzz.com/piao_tts/prd/scripts/base@ba2d4c123cb3074278981e0c8d9afcba.js" type="text/javascript" charset="utf-8"></script>
<script src="http://q.qunarzz.com/piao_tts/prd/scripts/cpanel/cpanel@b86a814a353d5f1ef0dc2fa2790e3245.js" type="text/javascript" charset="utf-8"></script>
<script src="http://q.qunarzz.com/js/qdatepicker/build/qdatepicker-0.1.min.js" type="text/javascript"></script>
<script src="http://q.qunarzz.com/piao_tts/prd/scripts/cpanel/list@9e2a8f0db9ae5d985ec6b9a99815548b.js" type="text/javascript" charset="utf-8"></script>
<script src="http://q.qunarzz.com/piao_tts/prd/scripts/cpanel/authorization/adminlistpage@62d7ca88296602e1cac8c4888c8c523e.js" type="text/javascript" charset="utf-8"></script>
<script type="text/javascript">
	window.context = {
		api: "/admin/authority"
	};
	(new tts.cpanel.authorization.AdminListPageInit(window.context)).init();
</script>
</body>
</html>
