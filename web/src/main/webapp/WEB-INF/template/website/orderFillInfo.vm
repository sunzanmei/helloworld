<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
	<meta http-equiv="content-type" content="text/html;charset=utf-8" />
	#if($collectionUtilsTool.isNotEmpty($dto.productSights))
		#foreach($sight in $dto.productSights)
			#if($!sightNames)
				#set($sightNames = $sightNames + $sight.sightName)
			#end
			#set($sightNames = $sight.sightName)
		#end
    #end
	<title>$!{dto.productMajorTitle} 门票订单填写</title>
	<meta name="keywords" content="$!{sightNames} $!{dto.productMajorTitle} $!{dto.productMinorTitle} 门票 去哪儿网" />
	<meta name="description" content="$!{dto.productMajorTitle} 门票订单填写，来去哪儿网预订景点门票，让您享受实实在在的优惠，去哪儿网是您快乐出游的更好选择！" />
	<link rel="stylesheet" type="text/css" href="http://q.qunarzz.com/piao_tts/prd/styles/ocenter/order_new@2eb5e426ded6bcfe936508871553a4bc.css" />
	<link href="http://user.qunar.com/webApi/popup.jsp?css" rel="stylesheet" type="text/css"/>
	#include('layout/headerInclude.vm')
</head>
<body>
<div class="tts_header" id="top">
	<div class="login">
		<div style="float:left;line-height:32px;white-space:nowrap;background:#f0f0f0"> <img src="http://source.qunar.com/piao/images/tts/ocenter/tips_logo.png" alt="qunar" style="position:relative;top:6px;float:left;margin-right:10px;vertical-align:top"><span>去哪儿网客户服务中心：10101234（如在境外拨打，请加拨0086）</span> </div>
		<ul id="user-info">
			#if($isLogin)
				<li id="user">去哪儿通行证</li>
				<li><a hidefocus="on" href="http://user.qunar.com/index.jsp#basic" target="_self">${userName}</a> |</li>
				<li id="linfo"><a hidefocus="on" href="http://order.qunar.com/ticket" target="_self">我的订单</a> |</li>
				<li id="logout"><a href="http://user.qunar.com/logout.jsp" title="退出" id="alogout">退出</a> |</li>
				<li><a href="http://help.qunar.com/" target="_top" style="color:#fe0053;">联系客服</a></li>
			#elseif($isThirdPartyLogin)
				<li><a hidefocus="on" href="http://user.qunar.com/index.jsp#basic" target="_blank" style="color: #ff7c00;">${exUserNickName}</a> |</li>
				<li id="linfo"><a hidefocus="on" href="http://order.qunar.com/ticket" target="_blank">我的订单</a> |</li>
				<li id="logout"><a href="http://user.qunar.com/logout.jsp" title="退出" id="alogout">退出</a> |</li>
			#else
				<li id="user">去哪儿通行证</li>
				<li id="login"><a href="http://user.qunar.com/passport/login.jsp" title="登录" id="alogin">登录</a> |</li>
				<li id="reg"><a href="http://user.qunar.com/passport/register.jsp" title="注册" id="areg">注册</a></li>
			#end
		</ul>
	</div>
    <script type="text/javascript">
        (function() {
            var ret = window.location.href;
            var keys = ["alogout", "alogin", "areg"];
            for(var i in keys) {
                var elem = document.getElementById(keys[i]);
                if(elem) {
                    elem.href += "?ret=" + encodeURIComponent(ret);
                }
            }
        })();
    </script>
</div>
<!-- 供应商 -->
<div class="supplier section clrfix">
	<div class="avata">
        #if($stringUtilsTool.isNotBlank($!dto.supplierWebsite))<a target="_blank" href="http://${dto.supplierWebsite}">#end
            <img src="${dto.supplierLogo}" alt="${dto.supplierShopName}" />
        #if($stringUtilsTool.isNotBlank($!dto.supplierWebsite))</a>#end
    </div>
	<div class="server_time">
		#if($stringUtilsTool.isNotBlank($!dto.supplierTelephone))<em>服务热线：${dto.supplierTelephone}</em>#end
		#if($stringUtilsTool.isNotBlank($!dto.supplierBusinessHour))<em>工作时间：${dto.supplierBusinessHour}</em>#end
	</div>
    <div class="tts_safe"><img src="http://simg2.qunarzz.com/piao/images/tts/ocenter/safe_logo.png" alt="全程预订保障，去哪儿都放心" /></div>
</div>
<!-- 定单正文 -->
<div class="tts_content">
<div class="section">
	#if(${dto.payWay}=="景区支付")
	<div class="steps  spot_payment">
		<ul class="clrfix">
			<li class="step_1 visited_pre">1.选择景点<i class="tab_status"></i></li>
			<li class="step_2 active">2.填写订单信息<i class="tab_status"></i></li>
			<li class="step_4">3.完成订单</li>
		</ul>
	</div>
	#else
	<div class="steps">
		<ul class="clrfix">
			<li class="step_1 visited_pre">1.选择景点<i class="tab_status"></i></li>
			<li class="step_2 active">2.填写订单信息<i class="tab_status"></i></li>
			<li class="step_3">3.在线支付<i class="tab_status"></i></li>
			<li class="step_4">4.完成订单</li>
		</ul>
	</div>
	#end
<div class="content_inner clrfix">
<div class="content_main">
	#if($dto.limitEnterSightType == "ADVANCE_BOOK_TIME")
	<div class="module_top_warn">
		<p><i class="icon i_warn"></i>预订限制：<b class="txt_red">请在游玩#if($dto.advanceDay==0)当天#else 前${dto.advanceDay}天#end的${dto.advanceTime}前完成预订</b></p>
	</div>
	#elseif($dto.limitEnterSightType == "DELAY_BOOK_HOURS")
	<div class="module_top_warn">
		<p><i class="icon i_warn"></i>预订限制：<b class="txt_red">请在入园前${dto.enterAfterBookHour}小时完成预订，否则可能造成入园不顺</b></p>
	</div>
	#end
<form action="${bookingUrl}" method="post" id="order-form">
	<input type="hidden" value="saveOrder" name ="method" />
	<input type="hidden" name="qunarPrice" id="qunarPrice" value="${dto.price}" />
	<input type="hidden" value="$!{productId}" id="productId" name="productId" />
	<input type="hidden" value="$!{tuanId}" name="tuanId" />
	<input type="hidden" value="" name="from" />
	<input type="hidden" value="" name="exTrack" />
	<input type="hidden" value="" name="cps" />
	<input type="hidden" value="" name="cpsInfo" />
	<input type="hidden" value="$!{orderSource}" name="orderSource" />
	<input type="hidden" value="$!{dto.priceId}" id="priceId" name="priceId" />
	<input type="hidden" value="$!{traceInfo}" id="traceInfo" name="traceInfo" />
	<input type="hidden" value="false" id="exchangePriceFlag" name="exchangePriceFlag" />
	<div class="content_major">
        <div class="content_major_info">
            <h1 class="major_name">$!{dto.productMajorTitle}</h1>
            <div class="major_notes">
                <span class="major_notes_tag major_notes_online">$!{dto.payWay}</span>
                #if($!dto.canRefund)
                <span class="major_notes_tag major_notes_refund" id="supportrefund" data-cardtarget="true">支持退款</span>
                #else
                <span class="major_notes_tag major_notes_none">&times;&nbsp;不支持退款</span>
                #end
                #if($!dto.passGuarantee)
                <span class="major_notes_tag major_notes_safe" id="guarantee" data-cardtarget="true">保障计划</span>
                #end
            </div>
            #if($dto.partner=="HOTEL" || $dto.partner == "SPU_TICKET_HOTEL")
                <div class="major_info_of">
                        <div class="mp_hotel_content">
                            <span class="mp_hotel_icon mp_icon_hotel"></span>
                            <div class="mo_hotel_detail">
                                <em>酒店</em>酒店：
                                <strong>$!{hotelDto.hotelName}$!{hotelDto.hotelRoomName}</strong>
                                <span>$!{hotelDto.hotelNightCount}晚</span>&nbsp;
                                <span>|</span>&nbsp;
                            <span>
                                #if(${hotelDto.breakfastCount}>0)
                                    #set($breakfastCount = ${TextUtils.convertSimplifiedArabicNumber2ChineseNumber(${hotelDto.breakfastCount})})
                                    #if(${breakfastCount} == "一")
                                        单早&nbsp;
                                    #elseif(${breakfastCount} == "二")
                                        双早&nbsp;
                                    #else
                                        ${breakfastCount}早&nbsp;
                                    #end
                                #else
                                    不含早
                                #end
                                #if(${hotelDto.internet}=="INTERNET_FREE")
                                    宽带免费
                                #elseif(${hotelDto.internet}=="INTERNET_COLLECT_FEES")
                                    宽带收费
                                #elseif(${hotelDto.internet}=="HAVE_NO_INTERNET")
                                    无宽带
                                #end
                                #if(${hotelDto.wifi}=="WIFI_FREE")
                                    wifi免费
                                #elseif(${hotelDto.wifi}=="WIFI_COLLECT_FEES")
                                    wifi收费
                                #elseif(${hotelDto.wifi}=="HAVE_NO_WIFI")
                                    无wifi
                                #end
                                #if(${hotelDto.hasWindow})
                                    有窗&nbsp;
                                #else
                                    无窗&nbsp;
                                #end
                            </span>
                            </div>
                        </div>
                        <div class="mp_hotel_content">
                            <span class="mp_hotel_icon mp_icon_sight"></span>
                            <div class="mo_hotel_detail">
                                <em>门票</em><span>$!{hotelDto.hotelTicketName}</span>
                                <span>$!{hotelDto.hotelTicketCount}张</span>
                            </div>
                        </div>
                    </div>
            #end
        </div>
        <div class="major_ticket_table">
            <div style="padding:10px 10px 5px">
                <table cellpadding="0" cellspacing="0" class="form_table">
                    <thead>
                    <tr>
                        #if($dto.teamType == "DATE")
                            #if($dto.partner=="HOTEL" || $dto.partner == "SPU_TICKET_HOTEL")
                                <th>入住日期</th>
                                <th>入园日期</th>
                            #else
                                <th width="250">游玩日期</th>
                            #end
                        #end
                        <th #if($dto.partner=="HOTEL" || $dto.partner == "SPU_TICKET_HOTEL") width="100"#else width="150"#end>门票单价</th>
	                    #if($dto.teamType != "DATE")
		                    <th>有效日期</th>
	                    #end
                        <th #if($dto.partner=="HOTEL" || $dto.partner == "SPU_TICKET_HOTEL") width="150"#else width="200"#end>#if($dto.partner=="HOTEL")套餐数量#{else}门票数量#end<i class="tag_haveto txt_red">*</i></th>
                        <th>总价</th>
                    </tr>
                    </thead>
                    <tbody>
                    <tr>
                        #if($dto.teamType == "DATE")
                            #if($dto.partner=="HOTEL" || $dto.partner == "SPU_TICKET_HOTEL")
                                <td>
                                    <div class="choose_date" style="width:200px;height:auto">
                                        <input type="text" value="" class="txt_input" id="useDate" placeholder="请选择入住日期" name="useDate" data-jvalidator-pattern="required">
                                        <label class="play_date_week" for="useDate" style="display: inline;"><span id="day-elem"></span><i class="icon i_calen"></i></label>
                                        <label class="error txt_red useDate_err" style="display: none;" for="useDate"><i class="icon i_error"></i>请选择入住日期</label>
                                    </div>
                                </td>
                                <td>
                                    <div class="label_form"><span id="ticketusecontainer">请先选择入住日期</span></div>
                                </td>
                            #else
                                <td>
                                    <div class="choose_date" style="width:230px;height:auto">
                                        <input type="text" value="" class="txt_input" id="useDate" placeholder="请选择游玩日期" name="useDate" data-jvalidator-pattern="required">
                                        <label class="play_date_week" for="useDate" style="display: inline;"><span id="day-elem"></span><i class="icon i_calen"></i></label><br>
                                        <label class="error txt_red useDate_err" style="display: none;" for="useDate"><i class="icon i_error"></i>请选择游玩日期</label>
                                        #if($dto.validUseDateDescription)
                                            <div id="usedatedescription" style="margin-top: 3px;">${dto.validUseDateDescription}</div>
                                        #end
                                    </div>
                                </td>
                            #end
                        #end
                        <td><strong class="price txt_orange">&yen;<em id="price-elem">$!{dto.price}</em></strong>
                            <div style="margin-top: 3px;">
	                            <span class="return_num" style="">
                                    <span class="return">返</span>
                                    <span class="price txt_orange">&yen;<em><input type="text" style="width:20px;margin:0" name="cashBackMoney" id="cashBackMoney" value="${dto.perCashBackMoney}" /></em></span></span>
                            #if($redPacketEnable)
                                <img src="http://simg2.qunarzz.com/piao/images/tts/chip/bonus.png" alt="红包支付" class="red-packet-icon" style="vertical-align: top;margin-top:-1px;">
                            #end
                            </div>
                        </td>
	                    #if($dto.teamType != "DATE")
		                    <td>
			                    #if($stringUtilsTool.isNotBlank($!dto.validUseDateDescription))
				                    <p><span>有效日期：</span>${dto.validUseDateDescription}</p>
			                    #end
			                    #if($stringUtilsTool.isNotBlank($!dto.invalidUseDateDescription))
				                    <p><span>不可使用日期：</span>${dto.invalidUseDateDescription}</p>
			                    #end
		                    </td>
	                    #end
                        <td>
                            <input type="text" class="txt_input" id="quantity" value="$!{dto.minBuyCount}" name="quantity" min="$!{dto.minBuyCount}" max="$!{dto.maxBuyCount}">
                            <div class="note txt_gray" id="minmax-desc">
                                #if($!{dto.minBuyCount} > 1)
                                最少可预订$!{dto.minBuyCount}张，
                                #end
                                最多可预订$!{dto.maxBuyCount}张
                            </div>
                        </td>
                        <td>
                            <strong class="price txt_orange">&yen;<em id="totalPrice"></em></strong>
                            <div class=" clrfix" id="cashback-container">

                            </div>
                        </td>
                    </tr
                    </tbody>
                </table>
                #if($stringUtilsTool.isNotBlank($!{dto.pickupTime}) || $stringUtilsTool.isNotBlank($!{dto.pickupAddress}))
                <div class="major_order_param">
                    #if($stringUtilsTool.isNotBlank($!{dto.pickupTime}))
                        <p><span>取票时间：</span>$!{dto.pickupTime}</p>
                    #end
                    #if($stringUtilsTool.isNotBlank($!{dto.pickupAddress}))
                        <p><span>取票地点：</span>$!{dto.pickupAddress}</p>
                    #end
                </div>
                #end
            </div>
        </div>
	</div>
    #if($!{dto.needPassenger})
        <h3 class="content_clasic">
            <strong>游客信息</strong>&nbsp;&nbsp;<a href="javascript:void(0)" hidefocus="true" id="loginpassenger" class="exp do_blue" style="display:none;">常用旅客</a>
            <span class="txt_gray">请填写任意<em id="passengerNumIn">1</em>位游客信息</span>
        </h3>
        <div class="content_detail">
            <div id="passengers"></div>
        </div>
    #end
    <h3 class="content_clasic">
        <strong>取票人信息</strong>&nbsp;&nbsp;<a href="javascript:void(0)" hidefocus="true" id="logincontact" class="exp do_blue" style="display:none;">常用取票人</a>
        <span class="txt_gray">用于接收门票信息和入园</span>
    </h3>
    <div class="content_detail">
        <div class="" id="contact">
            <table cellpadding="0" cellspacing="0" class="form_table">
                <thead>
                    #if($!{dto.needContactName})
                    <th width="160">联系人姓名</th>
                    #end
                    #if($!{dto.needContactPinyin})
                    <th width="160">姓名拼音</th>
                    #end
                    <th width="160">手机号</th>
                    #if($!{dto.needContactEmail})
                    <th width="160">邮箱</th>
                    #end
                    #if($!{dto.needContactPostalInfo})
                    <th width="160">地址</th>
                    <th width="100">邮编</th>
                    #end
                    <th></th>
                </thead>
                <tbody>
                    #if($!{dto.needContactName})
                    <td>
                        <input type="text" value="" class="txt_input placeholder" placeholder="请填写真实姓名" id="contactUser" name="contactUser" data-jvalidator-pattern="required&max_length[15]&min_length[2]" data-jvalidator-errMsg="2-15个汉字或字母" />
                        <label class="error txt_red contactUser_err" for="contactUser" style="display:none"><i class="icon i_error"></i>2-15个汉字或字母</label>
                    </td>
                    #end
                    #if($!{dto.needContactPinyin})
                    <td>
                        <input type="text" value="" class="txt_input placeholder" placeholder="请输入姓名拼音" id="contactUserPinyin" name="contactUserPinyin" data-jvalidator-pattern="required&pinyin&max_length[15]&min_length[2]" data-jvalidator-errMsg="请输入拼音" />
                        <label class="error txt_red contactUserPinyin_err" for="contactUserPinyin" style="display:none"><i class="icon i_error"></i>请正确输入拼音</label>
                    </td>
                    #end
                <td>
                    <input type="text" value="" class="txt_input placeholder" placeholder="用于接收门票信息" id="contactMobile" name="contactMobile" data-jvalidator-pattern="required&phone" data-jvalidator-errMsg="请输入正确的手机号码" />
                    <label class="error txt_red contactMobile_err" for="contactMobile" style="display:none"><i class="icon i_error"></i>请输入正确的手机号码</label>
                </td>
                    #if($!{dto.needContactEmail})
                    <td>
                        <input type="text" value="" class="txt_input placeholder" autocomplete="off" placeholder="请填写Email地址" id="contactEmail" name="contactEmail" data-jvalidator-pattern="required&email" data-jvalidator-errMsg="邮箱填写错误" />
                        <label class="error txt_red contactEmail_err" for="contactEmail" style="display:none"><i class="icon i_error"></i>邮箱填写错误</label>
                    </td>
                    #end
                    #if($!{dto.needContactPostalInfo})
                    <td>
                        <input type="text" value="" class="txt_input placeholder" placeholder="请输入地址" id="contactPostAddress" name="contactPostAddress" data-jvalidator-pattern="required" data-jvalidator-errMsg="请输入地址" />
                        <label class="error txt_red contactPostAddress_err" for="contactPostAddress" style="display:none"><i class="icon i_error"></i>请正确输入地址</label>
                    </td>
                    <td>
                        <input type="text" value="" style="width:70px;margin:0" class="txt_input placeholder" placeholder="请输入邮编" id="contactPostCode" name="contactPostCode" data-jvalidator-pattern="required" data-jvalidator-errMsg="请输入邮编" />
                        <label class="error txt_red contactPostCode_err" for="contactPostCode" style="display:none"><i class="icon i_error"></i>请正确输入邮编</label>
                    </td>
                    #end
                    <td></td>
                </tbody>
            </table>
        </div>
    </div>
    #if(${dto.bookNoticeFlag})
        <div class="content_buy_notes">
            <div class="content_buy_inner">
                <div class="buy_notes" id="attention">
                    <h3 class="caption"><i class="icon i_notes"></i>购买须知</h3>
                    #if($dto.canRefund || $stringUtilsTool.isNotBlank($!{dto.feeDescription}))
                        <h4 class="notes_title" style="color:#ff0073;">退票规则：</h4>
                    #end
                        <p class="notes_list" id="refunddesc">
                            #if($dto.canRefund)
                        #parse('include/module/refundDesc.vm')
                    #else
                        $!{dto.refundDescription}
                    #end
                    </p>
                    #if($!{dto.smstemp})
                        <h4 class="notes_title">取票及入园方式：</h4>
                        <p class="notes_list">
                            $!{dto.smstemp}
                        </p>
                    #end
                    #if($!{dto.useDescription})
                        <h4 class="notes_title">使用说明：</h4>
                        <p class="notes_list">
                            $!{dto.useDescription}
                        </p>
                    #end
                    #if($stringUtilsTool.isNotBlank($!{dto.feeDescription}))
                        <h4 class="notes_title">费用说明：</h4>
                        <p class="notes_list">
                            $!{dto.feeDescription}
                        </p>
                    #end
                    #if($stringUtilsTool.isNotBlank($!{dto.bookInfo}))
                        <h4 class="notes_title">预订须知：</h4>
                        <p class="notes_list">
                            $!{dto.bookInfo}
                        </p>
                    #end
                    #if($stringUtilsTool.isNotBlank($!{dto.feeInclude}))
                        <h4 class="notes_title">费用包含：</h4>
                        <p class="notes_list">
                            $!{dto.feeInclude}
                        </p>
                    #end
                    #if($stringUtilsTool.isNotBlank($!{dto.feeExclude}))
                        <h4 class="notes_title">费用不包含：</h4>
                        <p class="notes_list">
                            $!{dto.feeExclude}
                        </p>
                    #end
                </div>
            </div>
        </div>
    #end
    <div class="buy_agreement">
        <input type="checkbox" value="" checked="checked" id="agreement" />
        <label hidefocus="true" for="agreement">我已阅读并接受<a href="http://$!{dto.supplierWebsite}/shop/orderPolice" target="_blank" hidefocus="true" class="do_blue">合同条款、补充条款及其他所有内容</a></label>
    </div>
    <div class="form_unit clrfix" style="margin:20px 0; #if(!$dto.needCaptcha)display:none;#end">
        <label class="label_name" style="margin-right:auto;" for="captcha">验证码：</label>
        <input type="text" class="txt_input" name="captcha" id="captcha" style="width:inherit;">
    </div>
    <div class="form_control"><a id="confirm" href="javascript:void(0)" hidefocus="true"  class="btn btn_orange">提交订单</a></div>
</form>
</div>
<div class="content_side">
	<div class="side_section">
		<div class="side_content">
			#if($dto.partner=="HOTEL" || $dto.partner == "SPU_TICKET_HOTEL")
			<div class="side_belong" id="mapcontainer" style="height:150px;"></div>
			<dl id="message" class="intro clrfix"></dl>
			#else
