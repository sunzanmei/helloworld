<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta http-equiv="content-type" content="text/html;charset=utf-8" />
    <title>订单详情</title>
    <meta name="keywords" content="订单详情" />
    <meta name="description" content="订单详情" />
    <link rel="stylesheet" type="text/css" href="http://q.qunarzz.com/piao_tts/prd/styles/ocenter/order_detail@950e083416413ee3374f1406c1532cc6.css" />
    #include('layout/headerInclude.vm')
</head>
<body class="order_detial">

<!-- 供应商 -->
<div class="supplier section clrfix">
    <div class="avata">#if($!{dto.signType}==2 || ($!{dto.signType}==1 && $!{dto.qunarOperation}))去哪儿直销#elseif($!{dto.signType}==1)景区直销#{else}<img src="${dto.supplierLogo}" alt="${dto.supplierShopName}" />#end</div>
    <div class="server_time">
        #if($stringUtilsTool.isNotBlank($!dto.supplierTelephone))<em>服务热线：${dto.supplierTelephone}</em>#end
        #if($stringUtilsTool.isNotBlank($!dto.supplierBusinessHour))<em>工作时间：${dto.supplierBusinessHour}</em>#end
    </div>
</div>
<!-- 定单正文 -->
<div class="tts_content">
    <div class="section">
        <div class="content_inner clrfix">
            <div class="content_main">
                <div class="content_detail">
                    <div class="order_info_item">
                        <div class="full_caption light">订单信息</div>
                        <h1 class="pick_up">
                            $!{dto.title}
                            #if($!dto.passGuarantee)
                                <img src="http://simg2.qunarzz.com/piao/images/tts/ocenter/safe_icon.png" alt="保障计划" id="guarantee" class="icon_safeguard" />
                            #end
                            #if($!{dto.signType}==2 || ($!{dto.signType}==1 && $!{dto.qunarOperation}))
                                <b class="bold txt_red"> 去哪儿直销</b>
                            #elseif($!{dto.qunarOperation})
                                <strong class="tts_tags_inline" style="display: inline-block;border: 1px solid #ff4ba5;margin-left: 5px;padding: 3px 5px;line-height: 1.231;color: #ff4ba5;">Qunar代理运营</strong>
                            #end
                            #if($!{dto.needHotelCheck})
                                <span style="background:#FF3399;color:#fff;padding:1px 4px;font-weight: bold;">第二次确认</span>
                            #end
                            #if($stringUtilsTool.isNotBlank($!{dto.specificationDesc}))
                                <br />
                                <span style="color:#999">（$!{dto.specificationDesc}）</span>
                            #end
                        </h1>
                        <ul class="param_info_area clrfix">
                            <li> <span class="label">订单号码：</span> <em class="ellipsis" title="$!{dto.displayId}">$!{dto.displayId}</em> </li>
                            <li> <span class="label">产品id：</span> $!{dto.productId} </li>
                            <li> <span class="label">下单时间：</span> <em class="unit">$!dateTool.format("yyyy-MM-dd HH:mm",$!{dto.createTime})</em> </li>
                            <li> <span class="label">退款规则：</span> <span class="unit">
                                #if($!dto.canRefund)
                                    #if( ${dto.refundTag} == 0 )
                                        <b class="refund_status txt_green" id="supportrefund"><i class="icon i_succeed"></i>随心退</b>
                                    #elseif( ${dto.refundTag} == 1 )
                                        <b class="refund_status txt_green" id="supportrefund"><i class="icon i_succeed"></i>条件退</b>
                                    #else
                                        <b class="refund_status txt_green" id="supportrefund"><i class="icon i_succeed"></i>未知</b>
                                    #end
                                #else
                                    <b class="refund_status txt_gray"><i class="icon i_dont"></i>不可退款</b>
                                #end
                            </span></li>
                            <li>
                                <span class="label">购买数量：</span>
                                #if($stringUtilsTool.isNotBlank($!{dto.oldOrderQuantity}))
                                    <span class="ellipsis" title="$!{dto.oldOrderQuantity}">$!{dto.oldOrderQuantity}</span>
                                #else
                                    <span class="ellipsis">$!{dto.quantity}</span>
                                #end
                            </li>
                            <li> <span class="label">支付方式：</span> <span class="unit">$!{dto.payWay}</span></li>
                            <li>
                                <span class="label">订单状态：</span>
                                <span class="ellipsis">
                                #if(($!{dto.refundMoneyTrace} && ${dto.refundMoneyTrace.size()}>0) || ${dto.useQuantity}>0)
                                    <b class="bold txt_red has_des" data-tip-id="order-status">$!{dto.orderDisplayStatus}</b>
                                #else
                                    <b class="bold txt_red" data-tip-id="order-status">$!{dto.orderDisplayStatus}</b>
                                #end

                                    #if($!{dto.orderSue.sueStatus} != "UN_SUE")
                                        #if($!{dto.orderSue.sueStatus} == "SUE_REJECT")
                                            <b class="bold txt_red has_des" data-tip-id="sue-status">$!{dto.orderSue.sueStatus.displaySueStatus}</b>
                                        #else
                                            <b class="bold txt_black has_des" data-tip-id="sue-status">$!{dto.orderSue.sueStatus.displaySueStatus}</b>
                                        #end
                                    #end
                                </span>
                            </li>
                            <li> <span class="label">订单处理方式：</span> $!{dto.isManual} </li>

                            <li> <span class="label">支付流水号：</span> $!{dto.paySeq} </li>
                            <li> <span class="label">订单渠道：</span> $!{dto.orderSource} </li>
                            <li> <span class="label">订单来源：</span> $!{dto.orderFrom} </li>
                            <li> <span class="label">是否需要预约：</span> <script>document.write($!{dto.needReserve} ? "是" : "否");</script></li>

                            #if($!{dto.insuranceOrderNo})
                                <li style="width: 98%"> <span class="label">保险订单号：</span> $!{dto.insuranceOrderNo} </li>
                            #end
                            #if($!{dto.expectPrintTime})
                                <li> <span class="label">最晚确认时长：</span> $!{dto.expectPrintTime} </li>
                            #end

                            #if($!{dto.cardAmount} == 0)
                                <li style="width: 98%">

                                    <span class="label">订单金额：</span>
                                    <span class="unit price txt_orange">
                                        <em>
                                            <span class="yen_font">&yen;</span>$!numberTool.format($!math.sub($!{dto.origin},${dto.subsidyMoney}))
                                        </em>
                                    </span>

                                    #if(${dto.merchantSellMoney} != 0)
                                        <span class="unit price txt_orange">
                                        <em>&nbsp;=&nbsp;<span class="yen_font">&yen;</span>$!numberTool.format(${dto.merchantSellMoney})</em>
                                        <span class="zh_font">（商家售卖价）</span>
                                    </span>
                                    #end

                                    #if(${dto.forceQunarPrice} != 0)
                                    <span class="unit price txt_orange">
                                        <em>&nbsp;+&nbsp;<span class="yen_font">&yen;</span>$!numberTool.format(${dto.forceQunarPrice})</em>
                                        <span class="zh_font">（代金券）</span>
                                    </span>
                                    #end

                                    #if(${dto.expressFee} != 0)
                                    <span class="unit price txt_orange">
                                        <em>&nbsp;+&nbsp;<span class="yen_font">&yen;</span>$!numberTool.format(${dto.expressFee})</em>
                                        <span class="zh_font">（快递费）</span>
                                    </span>
                                    #end


                                    #if($!{dto.orderCashBackStatus} == "UN_CASH_BACK" || $!{dto.orderCashBackStatus} == "YET_TO_COME_BACK_TIME" )
                                        <span class="return_num" data-tip-type="cashback" data-tip-status="UN_CASH_BACK">
                                            <span class="return">返</span>
                                            <span class="price txt_orange">&yen;<em>$!{dto.orderCashBackMoney}</em></span>
                                        </span>
                                    #elseif($!{dto.orderCashBackStatus} == "ALREAD_CASH_BACK")
                                        <span class="return_num" data-tip-type="cashback" data-tip-status="ALREAD_CASH_BACK">
                                            <span class="return">已返</span>
                                            <span class="price txt_orange">&yen;<em>$!{dto.alreadyCashBackMoney}</em></span>
                                        </span>
                                    #elseif($!{dto.orderCashBackStatus} == "EXPIRE_CASH_BACK")
                                        <span class="return_num delay" data-tip-type="cashback" data-tip-status="EXPIRE_CASH_BACK">
                                            <span class="return">返</span>
                                            <span class="price txt_grey">&yen;<em>$!{dto.orderCashBackMoney}</em></span>
                                        </span>
                                    #end
                                </li>

                                <li style="width: 98%">
                                    <span class="label">实付金额：</span>
                                    <span class="unit price txt_orange">
                                        <em><span class="yen_font">&yen;</span>$!numberTool.format($!{dto.userOriginMoney})</em>
                                    </span>
                                    #if(${dto.redPacketMoney} > 0 || ${dto.couponMoney} > 0)
                                        <span class="unit price txt_orange">
                                            <em>&nbsp;=&nbsp;<span class="yen_font">&yen;</span>$!numberTool.format($!math.sub($!{dto.origin},${dto.subsidyMoney}))</em>
                                            <span class="zh_font">（订单金额）</span>
                                        </span>
                                        #if(${dto.redPacketMoney} > 0)
                                        <span class="unit price txt_orange">
                                            <em>&nbsp;-&nbsp;<span class="yen_font">&yen;</span>$!numberTool.format(${dto.redPacketMoney})</em>
                                            <span class="zh_font">（红包抵扣）</span>
                                        </span>
                                        #elseif(${dto.couponMoney} > 0)
                                        <span class="unit price txt_orange">
                                            <em>&nbsp;-&nbsp;<span class="yen_font">&yen;</span>$!numberTool.format(${dto.couponMoney})</em>
                                            <span class="zh_font">（代金券抵扣）</span>
                                        </span>
                                        #end
                                    #end
                                </li>
                            #end

                            #if($!{dto.carCoupon} == true)
                                <li style="width:98%;"> <span class="label">车车代金券：</span>
                                    $!{dto.carItemName}&nbsp;|&nbsp;$!{dto.carQuantity}张&nbsp;|&nbsp;￥$!{dto.carMoney}
                                </li>
                            #end
                        </ul>

                        #if($stringUtilsTool.isNotBlank($!{dto.orderDesc}))
                            <div class="order_info_item">
                                <div class="full_caption light">订单操作说明</div>
                                <ul class="param_info_list list_sign">
                                    <li><i class="i_dot_blue"></i><span>$!{dto.orderDesc}</span></li>
                                </ul>
                            </div>
                        #end
                        #if($!{dto.cardAmount} > 0)
                            <div class="order_pay_info">
                                <div class="clrfix"> <span class="label">订单金额：</span> <em class="unit price">&yen;<strong>$!numberTool.format($!math.sub($!{dto.origin},$!{dto.subsidyMoney}))</strong></em></div>
                                <div class="clrfix"> <span class="label">优惠金额：</span> <em class="unit price">-&yen;<strong>$!numberTool.format($!{dto.cardAmount})</strong></em> </div>
                                <div class="clrfix"> <span class="label">实付金额：</span> <em class="unit price txt_orange">&yen;<strong>$!numberTool.format($!{dto.money})</strong></em>
                                    #if($!{dto.orderCashBackStatus} == "UN_CASH_BACK" || $!{dto.orderCashBackStatus} == "YET_TO_COME_BACK_TIME" )
                                        <span class="return_num" data-tip-type="cashback" data-tip-status="UN_CASH_BACK"><span class="return">返</span><span class="price txt_orange">&yen;<em>$!{dto.orderCashBackMoney}</em></span></span>
                                    #elseif($!{dto.orderCashBackStatus} == "ALREAD_CASH_BACK")
                                        <span class="return_num" data-tip-type="cashback" data-tip-status="ALREAD_CASH_BACK"><span class="return">已返</span><span class="price txt_orange">&yen;<em>$!{dto.alreadyCashBackMoney}</em></span></span>
                                    #elseif($!{dto.orderCashBackStatus} == "EXPIRE_CASH_BACK")
                                        <span class="return_num delay" data-tip-type="cashback" data-tip-status="EXPIRE_CASH_BACK"><span class="return">返</span><span class="price txt_grey">&yen;<em>$!{dto.orderCashBackMoney}</em></span></span>
                                    #end
                                </div>
                            </div>
                        #end
                        <div class="order_info_item">
                            <div class="full_caption light">预订设置</div>
                            <ul class="param_info_list list_sign">
                                <li><i class="i_dot_blue"></i><span>提前预订时间：$!{dto.limitEnterSightDes}</span></li>
                                <li><i class="i_dot_blue"></i><span>订单取消设置：$!{dto.delayPayTimeDes}</span></li>
                            </ul>
                        </div>
                        #if($stringUtilsTool.isNotBlank($!dto.passMessagePreview))
                            <div class="order_info_item">
                                <div class="full_caption light">入园信息</div>
                                <ul class="param_info_list list_sign">
                                    <li><i class="i_dot_blue"></i><span>$!{dto.passMessagePreview}</span></li>
                                </ul>
                            </div>
                        #end
                    </div>

                    #if(!$null.isNull(${dto.productExtensionInfos}) && ${dto.productExtensionInfos.size()}>0)
                        <div class="order_info_item">
                            <div class="full_caption light">订单附加信息</div>
                            <table class="param_info_table">
                                <tbody>
                                    #foreach($pei in $!{dto.productExtensionInfos})
                                    <tr>
                                        <td width="15%"><b>$!{pei.name}:</b></td>
                                        <td width="85%">$!{pei.value}</td>
                                        <td></td>
                                    </tr>
                                    #end
                                </tbody>
                            </table>
                        </div>
                    #end

                    #if($!{hotelDto} || $!{childTicketOrderList})
                        <div class="order_info_item">
                            <div class="full_caption light">游玩信息</div>
                            <ul class="param_info_list">
                                #if($!{hotelDto})
                                    <li class="mp_hotel_wrap clrfix">
                                        <div class="mp_hotel_label" style="width:190px;">
                                            <span>酒店</span>
                                            <span style="margin-left:10px;"></span>
                                            <a href="$!{hotelUrl}" style="margin-left:10px;color:#34b9f2;" target="_blank">$!{hotelDto.hotelDisplayId}</a>
                                        </div>
                                        <div class="mp_hotel_content" style="margin-left:175px;">
                                            <span class="mp_hotel_icon mp_icon_hotel"></span>

                                            <div class="mo_hotel_detail">
                                                <strong>$!{hotelDto.hotelName}：</strong>
                                                <span>$!{hotelDto.stayDate}</span>&nbsp;
                                                <span>$!{hotelDto.hotelNightCount}晚</span>&nbsp;
                                                <span>|</span>&nbsp;
                                            <span>
                                                #if(${hotelDto.breakfastCount}>0)
                                                    含早&nbsp;
                                                #else
                                                    不含早&nbsp;
                                                #end
                                                #if(${hotelDto.wifi}=="WIFI_FREE")
                                                    免费wifi&nbsp;
                                                #elseif(${hotelDto.wifi}=="WIFI_COLLECT_FEES")
                                                    收费wifi&nbsp;
                                                #end
                                                #if(${hotelDto.internet}=="INTERNET_FREE")
                                                    免费宽带&nbsp;
                                                #elseif(${hotelDto.internet}=="INTERNET_COLLECT_FEES")
                                                    收费宽带&nbsp;
                                                #end
                                                #if(${hotelDto.hasWindow})
                                                    有窗&nbsp;
                                                #else
                                                    无窗&nbsp;
                                                #end
                                            </span>
                                            </div>
                                        </div>
                                    </li>
                                #end
                                #if($!{childTicketOrderList})
                                    #foreach($ticketItem in ${childTicketOrderList})
                                        <li class="mp_hotel_wrap clrfix">
                                            <div class="mp_hotel_label" style="width:190px;">
                                                <span>门票</span>
                                                <a href="$!{ticketUrl}&displayId=$!{ticketItem.displayId}" style="margin-left:10px;color:#34b9f2;"
                                                   target="_blank">$!{ticketItem.displayId}</a>
                                            </div>
                                            <div class="mp_hotel_content" style="margin-left:175px;">
                                                <span class="mp_hotel_icon mp_icon_sight"></span>

                                                <div class="mo_hotel_detail">
                                                    <strong>$!{ticketItem.productName}：</strong>
                                                    <span>$!{ticketItem.validUseDateDescription}</span>
                                                    <span>&nbsp;$!{ticketItem.quantity}张</span>
                                                    <span class="txt_orange">&nbsp;$!{ticketItem.displayStatus}</span>
                                                </div>
                                            </div>
                                        </li>
                                    #end
                                #end
                            </ul>
                        </div>

                    #else
                        <div class="order_info_item">
                            <div class="full_caption light">使用时间</div>
                            <ul class="param_info_list list_sign">
                                #if($stringUtilsTool.isNotBlank($!dto.validUseDateDescription))
                                    <li><i class="i_dot_blue"></i>有效日期：<span>$!{dto.validUseDateDescription}</span></li>
                                #end
                                #if($stringUtilsTool.isNotBlank($!dto.invalidUseDateDescription))
                                    <li><i class="i_dot_blue"></i>不可使用日期：<span>$!{dto.invalidUseDateDescription}</span></li>
                                #end
                                #if($stringUtilsTool.isNotBlank($!dto.lastRefundDate))
                                    <li><i class="i_dot_blue"></i>最晚退款时间：<span>$!{dto.lastRefundDate}</span></li>
                                #end
                            </ul>
                        </div>
                    #end
                    #if($dto.partner == "HOTEL" || $dto.partner == "SPU_TICKET_HOTEL")

                    #else

                    #end
                    #if(!$null.isNull(${dto.passengers}) && ${dto.passengers.size()}>0 && ${dto.needPassengerInfo})
                        <div class="order_info_item">
                            <div class="full_caption light">游客信息</div>
                            <table class="param_info_table">
                                <tbody>
                                <tr>
                                    #if(${dto.needPassengerName} )
                                        <th width="10%">姓名</th>
                                    #end
                                    #if(${dto.needPassengerPinyin} )
                                        <th width="14%">拼音</th>
                                    #end
                                    #if(${dto.needPassengerIdCard} )
                                        <th width="40%">证件</th>
                                    #end
                                    #if(${dto.needPassengerUserDefinei} )
                                        <th width="18%">$!{dto.passengerUserDefinedItemI}</th>
                                    #end
                                    #if(${dto.needPassengerUserDefineii} )
                                        <th width="18%">$!{dto.passengerUserDefinedItemII}</th>
                                    #end
                                </tr>
                                    #foreach($passenger in $!{dto.passengers})
                                    <tr>
                                        #if(${dto.needPassengerName} )
                                            <td>$!{passenger.name}</td>
                                        #end
                                        #if(${dto.needPassengerPinyin} )
                                            <td>$!{passenger.namePinyin}</td>
                                        #end
                                        #if(${dto.needPassengerIdCard} )
                                            <td><i class="icon i_man_id"></i><span>$!{passenger.idNo}</span></td>
                                        #end
                                        #if(${dto.needPassengerUserDefinei} )
                                            <td><p style="width:120px" class="ellipsis">$!{passenger.userDefineiValue}</p></td>
                                        #end
                                        #if(${dto.needPassengerUserDefineii} )
                                            <td><p style="width:120px" class="ellipsis">$!{passenger.userDefineiiValue}</p></td>
                                        #end
                                    </tr>
                                    #end
                                </tbody>
                            </table>
                        </div>
                    #end

                    <div class="order_info_item">
                        <div class="full_caption light">取票人信息<a style="display: none;" id="fetcher-button" href="javascript:void(0)" hidefocus="true" class="icon i_edit" title="修改取票人"></a></div>
                        <ul class="param_info_list list_sign">
                            #if($!{dto.contactUser})
                                <li><i class="i_dot_blue"></i>取票人：<span>$!{dto.contactUser}</span></li>
                            #end
                            #if($!{dto.contactUserPinyin})
                                <li><i class="i_dot_blue"></i>取票人拼音：<span>$!{dto.contactUserPinyin}</span></li>
                            #end
                            #if($!{dto.email})
                                <li><i class="i_dot_blue"></i>邮箱：<span>$!{dto.email}</span></li>
                            #end
                            <li><i class="i_dot_blue"></i>手机号：<span>$!{dto.contactMobile}</span></li>
                            #if($!{dto.userMessage})
                                <li><i class="i_dot_blue"></i>备注：<span>$!{dto.userMessage}</span></li>
                            #end
                        </ul>
                    </div>

                    #if(${dto.canResend} || $stringUtilsTool.isNotBlank($!{dto.pickUpTime}) || $stringUtilsTool.isNotBlank($!{dto.pickUpAddress}) )
                        <div class="order_info_item">
                            <div class="full_caption light">取票信息</div>
                            <ul class="param_info_list list_sign">
                                #if(${dto.canResend})
                                    <li> <i class="i_dot_blue"></i> $!{dto.supplierShopName}将为您发送入园凭证。如长时间未收到，请点击
                                        <a id="send-ticket" href="javascript:void(0)" hidefocus="true" class="do_blue">重新发送</a><span id="send-ticket-timer" class="txt_gray" style="display:none;">
                                    </span><br />或致电 <b class="txt_red">$!{dto.supplierTelephone}</b>
                                        <div>
                                            <i class="icon i_notes_secondony"></i><span class="gray">Qunar发码可以弹框修改手机号码，供应商发码的则不能修改手机号码，默认为取票人电话</span>
                                        </div>
                                    </li>

                                #end
                                #if($stringUtilsTool.isNotBlank($!{dto.pickUpTime})) <li> <i class="i_dot_blue"></i> 取票时间：$!{dto.pickUpTime} </li> #end
                                #if($stringUtilsTool.isNotBlank($!{dto.pickUpAddress})) <li> <i class="i_dot_blue"></i> 取票地点：$!{dto.pickUpAddress} </li> #end
                            </ul>
                            #if(${dto.resellType} == 2)
                                <div class="ticket-info-wrap">
                                    <ul class="ticket-info">
                                        #foreach($info in $dto.orderConsumeCodeInfo)
                                            <li class="ticket-info-item">
                                                <span class="item-label">电子票号:</span><span class="item-value">$!{info.consumeCode}</span>
                                                <span class="item-value">#if(${info.consumeStatus} == 1) 已使用 #else 未使用 #end</span>
                                            </li>
                                        #end
                                    </ul>
                                </div>
                            #end
                        </div>
                    #end

                    #if($stringUtilsTool.isNotBlank($!{dto.postAddress}) && $stringUtilsTool.isNotBlank($!{dto.postCode}) )
                        <div class="order_info_item">
                            <div class="full_caption light">邮寄地址</div>
