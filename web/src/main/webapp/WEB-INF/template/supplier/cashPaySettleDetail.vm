<!doctype html>
<html>
<head>
    <title>Qunar管理员后台</title>
    <meta name="keywords" content="Qunar管理员后台">
    <meta name="description" content="Qunar管理员后台">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <link rel="stylesheet" type="text/css" href="http://q.qunarzz.com/piao_tts/prd/styles/v2/list@12d870584660f131bd2807926611a4c7.css">
</head>
<body>
<div class="list_sort" style="width: 10px;"></div>
<div style="100%;text-align: center"><h3 class="caption">现付月结对账单</h3></div>
<div class="list_operate posi_top" style="text-align: left">
    <a href="javascript:void(0)" hidefocus="true" id="download-settlement" class="do do_blue"><i class="icon i_out"></i>导出本期账单</a>
</div>
<div class="list_cont" style="margin-top: 35px;">
    <h4 class="detail-title">账单汇总:</h4>
    <div class="detail-container">
        <table class="detail-table">
            <colgroup>
                <col width="200" />
                <col width="200" />
                <col width="100" />
                <col width="100" />
                <col width="100" />
                <col width="100" />
                <col width="100" />
            </colgroup>
            <thead>
            <tr>
                <th height="30">供应商名称</th>
                <th>账期</th>
                #if(${cashPaySettleInfo.sightStraightSign} == false && ${cashPaySettleInfo.qunarSendCode} == false)
                    <th>订单总金额</th>
                #else
                    <th>入园总金额</th>
                #end
                <th>应收佣金</th>
                <th>调整项金额</th>
                <th>实收金额</th>
            </tr>
            </thead>
            <tbody>
            <tr>
                <td>$!{cashPaySettleInfo.supplierName}</td>
                <td>$!{cashPaySettleInfo.settleDate}</td>
                #if(${cashPaySettleInfo.sightStraightSign} == false && ${cashPaySettleInfo.qunarSendCode} == false)
                    <td>$!{cashPaySettleInfo.sumOrderMoney}</td>
                #else
                    <td>$!{cashPaySettleInfo.sumConsumeMoney}</td>
                #end
                <td>$!{cashPaySettleInfo.qunarIncome}</td>
                <td>$!{cashPaySettleInfo.adjustMoney}</td>
                <td>$!{cashPaySettleInfo.realQunarIncome}</td>
            </tr>
            </tbody>
        </table>
    </div>
    #if( $!{settleDetailList} && $!{settleDetailList}!=0)
        <h4 class="detail-title">订单明细:</h4>
        <div class="detail-container">
            <table class="detail-table">
                <colgroup>
                    <col width="100" />
                    <col width="100" />
                    <col width="100" />
                    <col width="100" />
                    <col width="100" />
                    <col width="100" />
                    <col width="100" />
                    <col width="100" />
                    <col width="100" />
                    <col width="100" />
                    <col width="100" />
                    <col width="100" />
                </colgroup>
                <thead>
                <tr>
                    <th height="30">订单号</th>
                    <th>是否景区直签</th>
                    <th>是否Q发码</th>
                    <th>联系人</th>
                    <th>联系人电话</th>
                    <th>预订张数</th>
                    <th>单价</th>
                    <th>订单金额</th>
                    #if(${cashPaySettleInfo.sightStraightSign} == true || ${cashPaySettleInfo.qunarSendCode} == true)
                        <th>入园张数</th>
                        <th>入园金额</th>
                    #end
                    <th>应收佣金</th>
                    <th>用户入园时间</th>
                </tr>
                </thead>
                <tbody>
                    #foreach(${item} in ${settleDetailList})
                    <tr>
                        <td>$!{item.displayId}</td>
                        #if(${item.sightStraightSign}==true)
                            <td>是</td>
                        #else
                            <td>否</td>
                        #end
                        #if(${item.qunarSendCode}==true)
                            <td>是</td>
                        #else
                            <td>否</td>
                        #end
                        <td>$!{item.contactUser}</td>
                        <td>$!{item.contactMobile}</td>
                        <td>$!{item.quantity}</td>
                        <td>$!{item.unitPrice}</td>
                        <td>$!{item.originPrice}</td>
                        #if(${cashPaySettleInfo.sightStraightSign} == true || ${cashPaySettleInfo.qunarSendCode} == true)
                            <td>$!{item.consumeQuantity}</td>
                            <td>$!{item.consumeMoney}</td>
                        #end
                        <td>$!{item.qunarIncome}</td>
                        <td>$!{item.consumeTime}</td>
                    </tr>
                    #end
                </tbody>
            </table>
        </div>
    #end

    #if( $!{settleAdjustList} && $!{settleAdjustList}!=0)
        <h4 class="detail-title">调整数据:</h4>
        <div class="detail-container">
            <table class="detail-table">
                <colgroup>
                    <col width="400" />
                    <col width="100" />
                    <col width="100" />
                </colgroup>
                <thead>
                <tr>
                    <th height="30">调整原因</th>
                    <th>调整金额</th>
                    <th>调整时间</th>
                </tr>
                </thead>
                <tbody>
                    #foreach(${item} in ${settleAdjustList})
                    <tr>
                        <td>$!{item.adjustReason}</td>
                        <td>$!{item.adjustMoney}</td>
                        <td>$!{item.adjustDate}</td>
                    </tr>
                    #end
                </tbody>
            </table>
        </div>
    #end

</div>
<script src="http://q.qunarzz.com/jquery/prd/jquery-1.7.2.min.js" type="text/javascript" charset="utf-8"></script>
<script src="http://q.qunarzz.com/piao_tts/prd/scripts/base@ba2d4c123cb3074278981e0c8d9afcba.js" type="text/javascript" charset="utf-8"></script>
<script src="http://q.qunarzz.com/piao_tts/prd/scripts/cpanel/cpanel@b86a814a353d5f1ef0dc2fa2790e3245.js" type="text/javascript" charset="utf-8"></script>
<script src="http://q.qunarzz.com/js/qdatepicker/build/qdatepicker-0.1.min.js" type="text/javascript"></script>
<script src="http://q.qunarzz.com/piao_tts/prd/scripts/cpanel/list@9e2a8f0db9ae5d985ec6b9a99815548b.js" type="text/javascript" charset="utf-8"></script>
<script src="http://q.qunarzz.com/piao_tts/prd/scripts/cpanel/order/cashpayment/settlementdetailpage@de1d8e5b3a63a5467cad0e79a099d30f.js" type="text/javascript" charset="utf-8"></script>
<script type="text/javascript">
    window.context = {
        api: "/supplier/cashPaySettle",
        permission: "supplier",
        mainId: "$!{mainId}"
    };
    (new tts.cpanel.order.CashSettlementDetailPageInit(window.context)).init();
</script>
</body>
</html>
