<!doctype html>
<html>
<head>
    <title>Qunar管理员后台</title>
    <meta name="keywords" content="Qunar管理员后台">
    <meta name="description" content="Qunar管理员后台">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <link rel="stylesheet" type="text/css" href="http://q.qunarzz.com/piao_tts/prd/styles/v2/list@12d870584660f131bd2807926611a4c7.css">
</head>
<body>
<div class="tts_tab clrfix">
    <a href="/order/fax/hasBeenSendFaxList" class="tts_tab_item">已发送传真</a>
    <a href="/order/fax/toBeSendFaxList" class="tts_tab_item tts_tab_current">未发送传真</a>
</div>
<div class="list_condition">
    <div id="reminder-container"></div>
    <div class="search clrfix">
        <form id="search-form" method="post" action="/order/fax/queryToBeSendFaxList">
            <table style="table-layout: fixed;">
                <colgroup><col width="120" /><col width="200" /><col width="150" /><col width="" /></colgroup>
                <tr>
                    <td class="align_right"><label for="displayId" class="unit bold">订单号</label></td>
                    <td><input type="text" value="" class="fill w_l" id="displayId" name="displayId" /></td>
                    <td class="align_right"><label for="planSendStartDate" class="unit bold">传真计划发送时间</label></td>
                    <td>
                        <input type="text" name="planSendStartDate" id="planSendStartDate" class="fill w_l datepicker" value="" />
                        <input type="text" name="planSendEndDate" id="planSendEndDate" class="fill w_l datepicker" value="" />
                    </td>
                </tr>
                <tr>
                    <td class="align_right"><label for="faxName" class="unit bold">传真名称</label></td>
                    <td><input type="text" value="" class="fill w_l" id="faxName" name="faxName" /></td>
                    <td class="align_right"><label for="shopName" class="unit bold">店铺名称</label></td>
                    <td><input type="text" value="" class="fill w_l" id="shopName" name="shopName" /></td>
                </tr>
                <tr>
                    <td class="align_right"><label for="supplierName" class="unit bold">供应商名称</label></td>
                    <td><input type="text" value="" class="fill w_l" id="supplierName" name="supplierName" /></td>
                    <td class="align_right" colspan="2">
                        <a href="javascript:void(0)" hidefocus="true" id="sendbutton" class="btn btn_orange">批量发送</a>
                        <a href="javascript:void(0)" hidefocus="true" id="submitbutton" class="btn btn_orange">查询</a>
                    </td>
                </tr>
            </table>
        </form>
    </div>
</div>
<div class="list_cont">
    <div class="list_sort" style="width: 10px;"></div>
    </div>
<div id="list-container"></div>
</div>
<div id="pagination"></div>
</div>
<script src="http://q.qunarzz.com/jquery/prd/jquery-1.7.2.min.js" type="text/javascript" charset="utf-8"></script>
<script src="http://q.qunarzz.com/piao_tts/prd/scripts/base@ba2d4c123cb3074278981e0c8d9afcba.js" type="text/javascript" charset="utf-8"></script>
<script src="http://q.qunarzz.com/piao_tts/prd/scripts/cpanel/cpanel@b86a814a353d5f1ef0dc2fa2790e3245.js" type="text/javascript" charset="utf-8"></script>
<script src="http://q.qunarzz.com/js/qdatepicker/build/qdatepicker-0.1.min.js" type="text/javascript"></script>
<script src="http://q.qunarzz.com/piao_tts/prd/scripts/cpanel/list@9e2a8f0db9ae5d985ec6b9a99815548b.js" type="text/javascript" charset="utf-8"></script>
<script src="http://q.qunarzz.com/piao_tts/prd/scripts/cpanel/order/faxwaitlistpage@c4af5932e5d4ce9cfa1a5b57174e0d79.js" type="text/javascript" charset="utf-8"></script>
<script type="text/javascript">
    window.context = {
        listApi: "/order/fax",
        permission: "$!{permission}"
    };
    (new tts.cpanel.order.FaxWaitListPageInit(window.context)).init();
</script>
</body>
</html>
