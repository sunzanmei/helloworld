<?xml version="1.0" encoding="UTF-8"?>
<toolbox>
	<tool>
	     <key>math</key>
	     <scope>application</scope>
	     <class>org.apache.velocity.tools.generic.MathTool</class>
	</tool>

    <tool>
        <key>numberTool</key>
        <scope>application</scope>
        <class>org.apache.velocity.tools.generic.NumberTool</class>
    </tool>

	<tool>
	     <key>cookie</key>
	     <scope>request</scope>
	     <class>org.apache.velocity.tools.view.tools.CookieTool</class>
	</tool>
    <tool>
        <key>stringUtilsTool</key>
        <scope>request</scope>
        <class>org.apache.commons.lang.StringUtils</class>
    </tool>
    <tool>
        <key>collectionUtilsTool</key>
        <scope>request</scope>
        <class>org.apache.commons.collections.CollectionUtils</class>
    </tool>

    <tool>
        <key>TextUtils</key>
        <scope>request</scope>
        <class>com.qunar.piao.utils.TextUtils</class>
    </tool>
    <tool>
        <key>date</key>
        <scope>application</scope>
        <class>org.apache.velocity.tools.generic.DateTool</class>
    </tool>
    <tool>
        <key>shiro</key>
        <scope>application</scope>
        <class>com.qunar.piao.pms.client.common.support.velocity.Permission</class>
    </tool>
</toolbox>
